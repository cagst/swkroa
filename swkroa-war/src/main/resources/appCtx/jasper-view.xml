<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="membershipListingReport" class="org.springframework.web.servlet.view.jasperreports.JasperReportsMultiFormatView">
    <property name="url" value="classpath:reports/jasper/MembershipListingReport.jasper"/>
    <property name="jdbcDataSource" ref="datasource"/>
  </bean>

  <bean id="membershipPastDueReport" class="org.springframework.web.servlet.view.jasperreports.JasperReportsMultiFormatView">
    <property name="url" value="classpath:reports/jasper/MembershipPastDueReport.jasper"/>
    <property name="jdbcDataSource" ref="datasource"/>
  </bean>

</beans>
