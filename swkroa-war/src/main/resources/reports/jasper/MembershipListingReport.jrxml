	</parameter>
	<queryString>
		<![CDATA[SELECT iq3.membership_id
      ,iq3.fixed_dues_amount
      ,iq3.calculated_dues_amount
      ,iq3.member_count
      ,iq3.member_id
      ,iq3.company_name
      ,iq3.full_name
      ,iq3.membership_name
      ,iq3.membership_type_display
      ,iq3.owner_ident
      ,iq3.member_type_display
      ,iq3.dues_amount
      ,iq3.address_id
      ,a.address1
      ,a.address2
      ,a.address3
      ,a.city
      ,a.state_code
      ,a.postal_code
      ,a.country_code
  FROM (SELECT iq2.membership_id
              ,iq2.fixed_dues_amount
              ,iq2.calculated_dues_amount
              ,iq2.member_count
              ,iq2.member_id
              ,iq2.company_name
              ,iq2.full_name
              ,COALESCE(iq2.company_name, iq2.full_name) AS membership_name
              ,cv1.codevalue_display AS membership_type_display
              ,iq2.owner_ident
              ,mt.member_type_display
              ,mt.dues_amount
              ,iq2.address_id
         FROM (SELECT iq1.membership_id
                     ,iq1.membership_type_cd
                     ,iq1.fixed_dues_amount
                     ,iq1.calculated_dues_amount
                     ,iq1.member_count
                     ,m.member_id
                     ,m.company_name
                     ,CONCAT_WS(', ', p.name_last, p.name_first) AS full_name
                     ,m.member_type_id
                     ,m.owner_ident
                     ,MIN(a.address_id) AS address_id
                FROM (SELECT ms.membership_id
                            ,ms.membership_type_cd
                            ,ms.dues_amount AS fixed_dues_amount
                            ,SUM(mt.dues_amount) AS calculated_dues_amount
                            ,COUNT(m.member_id) AS member_count
                        FROM membership ms
                            ,member m
                            ,member_type mt
                       WHERE ms.active_ind = 1
                         AND (ms.membership_type_cd = $P{membership_type} OR $P{membership_type} = 0)
                         AND m.membership_id = ms.membership_id
                         AND m.active_ind = 1
                         AND mt.prev_member_type_id = m.member_type_id
                         AND mt.beg_eff_dt_tm < NOW() AND (mt.end_eff_dt_tm IS NULL OR mt.end_eff_dt_tm > NOW())
                       GROUP BY m.membership_id
                     )iq1
                     ,member m
                LEFT OUTER JOIN person p ON (p.person_id = m.person_id)
                LEFT OUTER JOIN address a ON (a.parent_entity_name = 'MEMBER' AND a.parent_entity_id = m.member_id AND a.active_ind = 1)
               WHERE m.membership_id = iq1.membership_id
                 AND m.active_ind = 1
               GROUP BY membership_id
                       ,membership_type_cd
                       ,fixed_dues_amount
                       ,calculated_dues_amount
                       ,member_count
                       ,member_id
                       ,company_name
                       ,full_name
                       ,member_type_id
                       ,owner_ident
             )iq2
             ,member_type mt
             ,codevalue cv1
       WHERE mt.prev_member_type_id = iq2.member_type_id
         AND mt.beg_eff_dt_tm < NOW() AND (mt.end_eff_dt_tm IS NULL OR mt.end_eff_dt_tm > NOW())
         AND cv1.codevalue_id = iq2.membership_type_cd
       )iq3
  LEFT OUTER JOIN address a ON (a.address_id = iq3.address_id)
ORDER BY iq3.membership_name]]>
