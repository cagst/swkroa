<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean name="userService" class="com.cagst.swkroa.user.UserServiceImpl">
    <constructor-arg ref="userRepo"/>
    <constructor-arg ref="roleRepo"/>
    <constructor-arg ref="securityService"/>
    <constructor-arg ref="contactRepo"/>
    <constructor-arg ref="passwordEncoder"/>
  </bean>

  <bean name="userUtils" class="com.cagst.swkroa.utils.UserUtilitiesImpl">
    <constructor-arg ref="userRepo"/>
  </bean>

  <bean name="membershipService" class="com.cagst.swkroa.member.MembershipServiceImpl">
    <constructor-arg ref="membershipRepo"/>
    <constructor-arg ref="contactRepo"/>
    <constructor-arg ref="commentRepo"/>
    <constructor-arg ref="transactionRepo"/>
  </bean>
</beans>